{
  "name": "fulcrum-schema",
  "version": "2.0.1",
  "description": "Table schemas for Fulcrum apps",
  "main": "./index.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/fulcrumapp/fulcrum-schema.git"
  },
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "npm run lint && node_modules/.bin/mocha ./test/setup.js test/test.js",
    "prepublish": "npm run lint && npm run clean && npm run build",
    "lint": "node_modules/.bin/eslint src/*.js src/**/*.js",
    "build": "node_modules/.bin/babel src --out-dir dist --source-maps && browserify dist/postgres.js > dist/postgres.min.js --mangle",
    "clean": "rm -rf dist"
  },
  "author": "Zac McCormick <zac.mccormick@gmail.com> (http://github.com/zhm)",
  "license": "BSD",
  "eslintConfig": {
    "extends": "fulcrum"
  },
  "dependencies": {
    "babel-polyfill": "^6.9.0",
    "sqldiff": "^0.0.21",
    "underscore": "^1.8.3"
  },
  "devDependencies": {
    "babel-cli": "^6.9.0",
    "babel-core": "^6.9.0",
    "babel-eslint": "^6.0.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-1": "^6.5.0",
    "chai": "^3.5.0",
    "chai-as-promised": "^5.3.0",
    "eslint": "^2.10.2",
    "eslint-config-fulcrum": "^1.0.2",
    "eslint-plugin-react": "^5.1.1",
    "fs-sync": "^1.0.2",
    "mocha": "^2.4.5",
    "season": "^5.3.0",
    "should": "^8.3.2"
  }
}
