{
  "name": "@fulcrumapp/fulcrum-schema",
  "version": "3.6.1",
  "description": "Table schemas for Fulcrum apps",
  "main": "fulcrum-schema.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/fulcrumapp/fulcrum-schema.git"
  },
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "mocha ./test/setup.js test/test-postgres.js test/test-sqlite.js test/test-v2-to-v3.js test/test-v3-to-v4.js --reporter-option maxDiffSize=20000",
    "lint": "eslint src/*.js src/**/*.js",
    "build": "cross-env BABEL_ENV=production babel src -d dist -s && cp package.json dist && browserify dist/fulcrum-schema.js > dist/fulcrum-schema.min.js --mangle",
    "clean": "rm -rf dist"
  },
  "author": "Zac McCormick <zac.mccormick@gmail.com> (http://github.com/zhm)",
  "license": "BSD",
  "eslintConfig": {
    "extends": "fulcrum"
  },
  "dependencies": {
    "@babel/polyfill": "^7.12.1",
    "lodash": "^4.17.21",
    "sqldiff": "^0.3.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.13.16",
    "@babel/core": "^7.13.16",
    "@babel/eslint-parser": "^7.13.14",
    "@babel/eslint-plugin": "^7.13.16",
    "@babel/plugin-transform-modules-commonjs": "^7.13.8",
    "@babel/preset-env": "^7.13.15",
    "@babel/register": "^7.13.16",
    "browserify": "^17.0.0",
    "chai": "^4.3.4",
    "chai-as-promised": "^7.1.1",
    "cross-env": "^7.0.3",
    "eslint": "^7.25.0",
    "eslint-config-fulcrum": "^2.0.1",
    "eslint-plugin-react": "^7.23.2",
    "fs-sync": "^1.0.6",
    "mocha": "^9.1.1",
    "season": "^6.0.2",
    "should": "^13.2.3"
  }
}
